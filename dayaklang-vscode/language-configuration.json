{
  "comments": { "lineComment": "//" },
  "brackets": [["{","}"], ["[","]"], ["(",")"]],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"" }
  ],
  "surroundingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"" }
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(misal\\b.*|.*\\{)\\s*$",
    "decreaseIndentPattern": "^\\s*\\}"
  }
}
